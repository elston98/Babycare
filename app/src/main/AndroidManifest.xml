<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wilson.elston.babycare">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/main_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/main_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver android:name=".AlarmReceiver" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".baby_guide"
            android:label="baby_guide"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".second"
            android:label="@string/title_activity_second"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".third"
            android:label="@string/title_activity_third"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".fourth"
            android:label="@string/title_activity_fourth"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".fifth"
            android:label="@string/title_activity_fifth"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".vaccination"
            android:label="@string/title_activity_sixth"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".growth_diary"
            android:label="growth_diary"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Diary"
            android:parentActivityName=".growth_diary" />
        <activity
            android:name=".content"
            android:parentActivityName=".growth_diary" />
        <activity android:name=".Details" />
        <activity android:name=".ImagesActivity" />
        <activity android:name=".Appointment" />
        <activity android:name=".Profile"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>